{
    "workbench.startupEditor": "welcomePage",
    "workbench.colorTheme": "Visual Studio Light",
    "editor.fontFamily": "Monospace,'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.tabSize": 3,
    "editor.renderWhitespace": "boundary",
    "editor.wordWrap": "on",
    "editor.mouseWheelZoom": true,
    "editor.minimap.maxColumn": 60,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 3, TabWidth: 3, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -3 }",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.includePath": [
        "${default}",
        "/opt/ibm/ILOG/CPLEX_Studio129/cplex/include/",
        "/opt/ibm/ILOG/CPLEX_Studio129/concert/include/"
    ],
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.view.pdf.external.viewer.command": "okular",
    "latex-workshop.view.pdf.external.viewer.args": [
        "-unique",
        "%PDF%"
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.ist",
        "*.fls",
        "*.log",
        "*.fdb_latexmk",
        "*.snm",
        "*.nav",
        "*.synctex.gz"
    ],
    "C_Cpp.updateChannel": "Insiders"
}